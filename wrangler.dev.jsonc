		"PERSONAS_DOMAIN": "localhost:8081",
		"PERSONAS_URI": "http://localhost:8081",
		"HOLDING_VERIFIER_PUBLIC_KEY": "0x67e81DE7802A5f7efEF66b156F2d06a526Bd5BD6",
